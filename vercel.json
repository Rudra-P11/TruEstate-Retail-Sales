{
  "buildCommand": "npm run build --workspace=frontend",
  "outputDirectory": "frontend/dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "env": {
    "VITE_API_URL": "@vite_api_url"
  },
  "regions": ["iad1"],
  "functions": {
    "frontend/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  }
}
